{
  "name": "laravel-auth-mcp-server",
  "version": "1.0.1",
  "description": "A Laravel Authentication Model Context Protocol (MCP) server implementation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "laravel-auth-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "laravel",
    "authentication",
    "auth",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
